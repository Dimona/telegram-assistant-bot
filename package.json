    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "is-json": "^2.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "4.17.21",
    "nestjs-cls": "^5.4.0",
    "nestjs-telegraf": "^2.8.1",
    "openai": "^4.85.4",
    "patch-package": "^8.0.0",
    "qs": "^6.14.0",
